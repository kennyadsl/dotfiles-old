ZSH=$HOME/.oh-my-zsh
ZSH_THEME="miloshadzic"

alias zshconfig="st ~/.zshrc"
alias pulsar="pulsar cap -c git@github.com:nebulab/pulsar-conf.git"
alias be="bundle exec"
alias bo="bundle open"
alias bs="bundle show"
alias rvm@="rvm gemset use `basename $PWD`"

plugins=(bundler git github osx ruby rails3 rvm sublime)

source $ZSH/oh-my-zsh.sh

export PATH=$PATH:/usr/bin:/bin:/usr/sbin:/sbin:/usr/local/bin:~/.rvm/bin
export EDITOR=subl

cdpath=(~/Code/)
